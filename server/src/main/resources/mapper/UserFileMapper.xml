<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.file.mapper.UserFileMapper">
    <insert id="insertUserFile" parameterType="com.example.file.domain.UploadTask">
        insert into userfile (file_size,extend,file_name,file_id,user_id,status)
        value (#{fileSize},#{extend},#{fileName},#{id},#{userId},#{status});
    </insert>
</mapper>